{
    "name": "digitalastronauts/warp",
    "description": "Simple database layer based on `nette/database` to work effectively with entities.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Krizan",
            "email": "mnohosten@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "nette/database": "^3.1",
        "symfony/property-access": "^5.3",
        "robmorgan/phinx": "^0.12.9",
        "nette/php-generator": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Warp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Warp\\Tests\\": "tests"
        }
    },
    "bin": ["bin/warp"]
}
